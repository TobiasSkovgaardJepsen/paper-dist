AX[AX[P0 <= 2 & 2 <= P0]];
AG[AX[ [P0 <= 2 & 2 <= P0] | [ 1 <= P1 | P1 <= 1 ] | [ P1 <= 2 & 2 <= P1 ] ]];

EX[[ 1 <= P1 | P1 <= 1 ] | [ P1 <= 2 & 2 <= P1 ] ];
EF[EX[ P1 <= 0 & P0 <= 1 ]];

AF[ P1 <= 0];
AG[AF[ P1 <= 0 ]];  
EF[ P0 <= 0 & 0 <= P0];
EF[3 <= P1];

E [P0 <= 0 U [ P1 <= 1 & 1 <= P1]];
E [P0 <= 2 U 3 <= P1];


A [P0 <= 0 U [ P1 <= 1 & 1 <= P1]];
A [P0 <= 2 U 3 <= P1];