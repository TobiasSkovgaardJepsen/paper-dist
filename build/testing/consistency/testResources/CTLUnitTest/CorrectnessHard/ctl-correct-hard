AX[2 <= Ready1];
AX[AX[AX[1 <= Done]]];

EX[2 <= Ready1 | 2 <= Ready2];
EX[1 <= Done];

AF[2 <= Done];
AF[2 <= Ready2];

EF[1 <= Ready1 & 1 <= Ready2];
EF[1 <= Ready1 & 1 <= Done];

AF[A[ 1 <= Ready1 U 2 <= Done]];
[A[ 1 <= Ready1 U 1 <= AgreeOnSender]];
EG[A[ 1 <= AgreeOnSender | 1 <= Ready1 U 1 <= Done ]];

EX[E [ 1 <= Ready1 U 1 <= Done]];
[E[ 1 <= Ready1 U 1 <= AgreeOnSender]];
EG[E[ 1 <= AgreeOnSender | 1 <= Ready1 U 1 <= Done ]];